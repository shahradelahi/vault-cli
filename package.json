{
  "name": "kvault",
  "description": "CLI for HashiCorp's Vault Key/Value V2 engine",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "kvault": "dist/cli.js"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "pnpm typecheck && eslint .",
    "lint:fix": "eslint --fix .",
    "format:check": "prettier --check \"**/*.{ts,md}\"",
    "format": "prettier --write .",
    "prepublishOnly": "pnpm format:check && pnpm lint && pnpm build"
  },
  "packageManager": "pnpm@9.6.0",
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "@litehex/node-vault": "1.0.0-canary.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "execa": "^9.3.0",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^6.0.0",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "tsup": "^8.2.2",
    "type-fest": "^4.23.0",
    "typescript": "^5.5.4"
  },
  "license": "GPL-3.0",
  "author": "Shahrad Elahi <shahrad@litehex.com> (https://github.com/shahradelahi)",
  "repository": {
    "type": "git",
    "url": "https://github.com/shahradelahi/vault-cli.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vault",
    "cli",
    "kv-v2",
    "hashicorp",
    "secrets"
  ]
}
