{
  "name": "kvault",
  "version": "0.2.0",
  "description": "CLI for HashiCorp Vault Key/Value V2 secret engines",
  "keywords": [
    "vault",
    "cli",
    "kv-v2",
    "key-value",
    "hashicorp",
    "secrets"
  ],
  "homepage": "https://github.com/shahradelahi/vault-cli",
  "repository": "github:shahradelahi/vault-cli",
  "license": "MIT",
  "author": "Shahrad Elahi <shahrad@litehex.com> (https://github.com/shahradelahi)",
  "type": "module",
  "bin": {
    "kvault": "dist/cli.js"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "pnpm typecheck && eslint .",
    "lint:fix": "eslint --fix .",
    "prepublishOnly": "pnpm format:check && pnpm lint && pnpm build",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@shahrad/prettier-config",
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "@litehex/node-vault": "^1.0.4",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "dotenv": "^17.2.2",
    "execa": "^9.6.0",
    "ora": "^9.0.0",
    "prompts": "^2.4.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@shahrad/eslint-config": "^1.0.1",
    "@shahrad/prettier-config": "^1.2.2",
    "@shahrad/tsconfig": "^1.2.0",
    "@types/prompts": "^2.4.9",
    "eslint": "^9.37.0",
    "prettier": "^3.6.2",
    "testcontainers": "^10.16.0",
    "tsup": "^8.5.0",
    "type-fest": "^4.41.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.18.3+sha512.bbd16e6d7286fd7e01f6b3c0b3c932cda2965c06a908328f74663f10a9aea51f1129eea615134bf992831b009eabe167ecb7008b597f40ff9bc75946aadfb08d",
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
